{
  "name": "docs",
  "description": "Documentation chameleon, adapts to any language and will help you document all the things.",
  "author": "Tyler Benton <tjbenton21@gmail.com>",
  "version": "0.0.0beta1",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "compile": "npm run clean; babel app/ -d ./dist --optional runtime --stage 0",
    "watch": "npm run clean; babel app/ -d ./dist --stage 0 --optional runtime --watch",
    "test": "tape dist/tests/**/*.js | tap-spec",
    "coverage": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "plato": "plato -d coverage/plato dist/index.js",
    "deploy": "npm run test && git push origin master && npm publish",
    "prepublish": "npm run compile"
  },
  "nyc": {
    "exclude": [
      "node_modules/",
      "*/utils/fs.js"
    ]
  },
  "files": [
    "dist/",
    "bin/",
    "LICENSE"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjbenton/docs.git"
  },
  "bugs": {
    "url": "https://github.com/tjbenton/docs/issues"
  },
  "bin": {
    "docs": "bin/docs"
  },
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "chalk": "^1.1.0",
    "change-case": "^2.3.0",
    "co": "^4.6.0",
    "commander": "^2.9.0",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.6",
    "is_js": "^0.7.4",
    "marked": "^0.3.3"
  },
  "keywords": [
    "css",
    "sass",
    "scss",
    "less",
    "stylus",
    "crush",
    "preprocessor",
    "js",
    "javascript",
    "coffescript",
    "sql",
    "nosql",
    "perl",
    "java",
    "coldfusion",
    "xml",
    "cfm",
    "cfc",
    "cfml",
    "php",
    "c",
    "c#",
    "c++",
    "objective-c",
    "swift",
    "python",
    "ruby",
    "rb",
    "shell",
    "bash",
    "code",
    "doc",
    "docs",
    "documentation",
    "parse",
    "parser",
    "annotation",
    "comments"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "coveralls": "^2.11.4",
    "nyc": "^3.2.2",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.1",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.1"
  }
}
