  "author": "Tyler Benton",
  "version": "0.0.0beta1",
  "scripts": {
    "test": "istanbul test | tape tests/**/* | tap-spec",
    "watch": "rm -rf ./dist; babel app/ -d ./dist --stage 0 --optional runtime --watch",
    "deploy": "npm run test && git push origin master && npm publish",
    "build": "rm -rf ./dist; babel app/ -d ./dist --optional runtime --stage 0",
    "prepublish": "npm run build"
  },
